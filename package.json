{
  "name": "elm-cacay",
  "description": "Cosku Acay's personal website",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Cosku Acay",
  "repository": {
    "type": "git",
    "url": "https://github.com/cacay/elm-cacay"
  },
  "scripts": {
    "start": "webpack-dev-server --hot --inline --content-base src/",
    "build": "rimraf dist && webpack",
    "watch": "webpack --watch",
    "postinstall": "rsync -r node_modules/semantic-ui-less/themes node_modules/semantic-ui-less/definitions node_modules/semantic-ui-less/theme.less node_modules/semantic-ui-less/semantic.less src/styles/ && find src/styles -type f -exec chmod -x {} \\; && elm package install -y"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "~2.10.2",
    "eslint-loader": "^1.3.0",
    "semantic-ui-less": "^2.2.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "elm": "^0.17.0",
    "elm-hot-loader": "^0.3.2",
    "elm-webpack-loader": "^3.0.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.17.0",
    "less": "^2.7.1",
    "postcss-loader": "^0.9.1",
    "rimraf": "^2.5.2",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "less-plugin-rewrite-import": "^0.1.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1"
  }
